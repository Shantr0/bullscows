<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>BullsCowsHttpServlet</servlet-name>
    <servlet-class>main.java.BullsCowsHttpServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddUser</servlet-name>
    <servlet-class>main.java.AddUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BullsCowsHttpServlet</servlet-name>
    <url-pattern>/BullsCowsHttpServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddUser</servlet-name>
    <url-pattern>/AddUser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>NewGame</servlet-name>
    <servlet-class>main.java.NewGame</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NewGame</servlet-name>
    <url-pattern>/NewGame</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Number</servlet-name>
    <servlet-class>main.java.Number</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Number</servlet-name>
    <url-pattern>/Number</url-pattern>
  </servlet-mapping>
</web-app>
